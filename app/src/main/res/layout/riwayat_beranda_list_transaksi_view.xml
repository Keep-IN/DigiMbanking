<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iv_transaksi_masuk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_masuk" />

    <TextView
        android:id="@+id/tv_transaksi_masuk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Transfer Masuk"
        app:layout_constraintBottom_toBottomOf="@+id/iv_transaksi_masuk"
        app:layout_constraintStart_toEndOf="@+id/iv_transaksi_masuk"
        app:layout_constraintTop_toTopOf="@+id/iv_transaksi_masuk" />

    <TextView
        android:id="@+id/tv_nominal_riwayat_transaksi_masuk_beranda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:text="+ Rp4.500.000"
        android:textColor="@color/green_700"
        app:layout_constraintBottom_toBottomOf="@+id/iv_transaksi_masuk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_transaksi_masuk" />

</androidx.constraintlayout.widget.ConstraintLayout>